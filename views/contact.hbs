    <section class="contact-section section-padding">
            <div class="container">
                <div class="section-title-s4">
                    <h2>Get a Quote Here</h2>
                </div>
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-12">
                        <div class="hs-responsive-embed-youtube"><iframe
                                src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d60298.37866642666!2d73.0272286!3d19.1668503!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3be7bf6d15555555%3A0x487fb5a9cb4503d6!2sAscent%20Porta%20Solutions!5e0!3m2!1sen!2sin!4v1691150602636!5m2!1sen!2sin"
                                width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"
                                referrerpolicy="no-referrer-when-downgrade"></iframe></div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-12 mt-5">
                        <div class="contact-form">
                            <form method="post" id="contact-form" class="form row contact-validation-active">
                                <div class="col-lg-6 col-md-6 col-sm-12">
                                    <label for="name">Full Name</label>
                                    <input type="text" class="form-control" id="name" name="name">
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-12">
                                    <label for="email">Email</label>
                                    <input type="email" class="form-control" id="email" name="email">
                                </div>
                                <div class="col-lg-6 col-md-6 col-sm-12">
                                    <label for="phone">Phone No</label>
                                    <input type="text" class="form-control" id="phone" name="phone">
                                </div>
                                <div class="col-lg-12 col-md-12 col-sm-12">
                                    <label for="business">Message</label>
                                    <textarea class="form-control" name="message" cols="90" rows="5"></textarea>
                                </div>
                                <div class="col col-xs-12 submit-btn">
                                    <button type="submit">Submit</button>
                                    <div id="loader">
                                        <i class="fa fa-refresh fa-spin fa-3x fa-fw"></i>
                                    </div>
                                </div>
                                <div class="error-handling-messages">
                                    <div id="success">Thank you</div>
                                    <div id="error"> Error occurred while sending email. Please try again later. </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div> <!-- end row -->
            </div> <!-- end container -->
        </section>
<!-- end contact-pg-content -->

